   "source": [
    "import numpy as np\n",
    "X = np.array([[6,7], [5,6], [7,4], [6,4], [8,6], [9,5]])\n",
    "y = np.array([1, 1, 1, 1, 0, 0])\n",
    "x_new = [7,7]\n",
    "def predict_1NN(X, y, x_new):\n",
    "    ind = np.argmin(np.linalg.norm(X-x_new, axis=1))\n",
    "    return y[ind]"
   ]
